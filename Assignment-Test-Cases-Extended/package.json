{
  "name": "assignment-test-cases",
  "version": "1.0.0",
  "description": "A web interface for testing JavaScript functions with Jest",
  "main": "public/js/server.js",
  "scripts": {
    "test": "jest",
    "start": "node public/js/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jest": "^29.7.0"
  },
