    "body-parser": "^1.20.2",
    "express": "^4.18.2"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/public/js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  }
}
